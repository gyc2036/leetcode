class Solution 
{
    public:
    int counting(int n,int d)
    {
        long long int result=0;
        for(long long int i=1;i<=n;i*=10) result+=(n/i+(9-((d==0)?10:d)))/10*i+(((n/i)%10==d)?(n%i+1):0);
        return result;
    }
    int digitsCount(int d, int low, int high) 
    {
        return counting(high,d)-counting(low-1,d);
    }
};
